<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/paddingExterno"
    tools:context="com.example.moacir.moacirmodernartui.MainActivity"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/internalLayout01"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="15">

        <LinearLayout
            android:id="@+id/internalLayout0101"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:orientation="vertical"
            android:layout_alignParentLeft="true">

            <TextView
                android:id="@+id/textView01"
                android:text="@string/textViewText"
                android:gravity="center_vertical|center"
                android:textSize="20sp"
                android:textColor="@color/textColorBlack"
                android:background="@color/textView01Color"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="@dimen/margemEntreBlocos"/>

            <TextView
                android:id="@+id/textView02"
                android:text="@string/textViewText"
                android:gravity="center_vertical|center"
                android:textSize="20sp"
                android:textColor="@color/textColorBlack"
                android:background="@color/textView02Color"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_margin="@dimen/margemEntreBlocos"/>
        </LinearLayout>

        <LinearLayout
            android:id="@+id/iternalLayout0102"
            android:layout_width="0sp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView03"
                android:text="@string/textViewText"
                android:gravity="center_vertical|center"
                android:textSize="20sp"
                android:textColor="@color/textColorBlack"
                android:background="@color/textView03Color"
                android:layout_width="match_parent"
                android:layout_height="0sp"
                android:layout_weight="1"
                android:layout_margin="@dimen/margemEntreBlocos"/>
            <TextView
                android:id="@+id/textView04"
                android:text="@string/textViewText"
                android:gravity="center_vertical|center"
                android:textSize="20sp"
                android:textColor="@color/textColorBlack"
                android:background="@color/textView04Color"
                android:layout_width="match_parent"
                android:layout_height="0sp"
                android:layout_weight="1"
                android:layout_margin="@dimen/margemEntreBlocos"/>
            <TextView
                android:id="@+id/textView05"
                android:text="@string/textViewText"
                android:gravity="center_vertical|center"
                android:textSize="20sp"
                android:textColor="@color/textColorBlack"
                android:background="@color/textView05Color"
                android:layout_width="match_parent"
                android:layout_height="0sp"
                android:layout_weight="1"
                android:layout_margin="@dimen/margemEntreBlocos"/>
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:id="@+id/internalLayout02"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">

        <SeekBar
            android:id="@+id/seekBar"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
    </LinearLayout>
</LinearLayout>